/**index.wxss**/
.Container{
  min-height : 100vh;
  background: #F8F8F8;
  padding:14px;
  position:relative;
}
.dynamiclist{
  width:100%;
}
.dynamiclist .synamicitem{
  background:#fff;
  margin-bottom:10px;
  padding:12px 20px;
  border-radius:5px;
}
.synamicitem .title{
  font-size:11px;
  color:#BEBEBE;
  line-height:100%;
  padding-bottom:5px;
}
.synamicitem .text{
  font-size:13px;
  color:#333333;
  line-height:18px;
}
